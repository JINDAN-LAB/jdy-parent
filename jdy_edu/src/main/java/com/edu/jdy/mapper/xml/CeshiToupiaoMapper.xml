<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.edu.jdy.mapper.CeshiToupiaoMapper">

<!--    seletelist-->
    <select id="seletelist" resultType="com.edu.jdy.entity.CeshiToupiao">
         SELECT count(xuhao) as phones , xuhao FROM ceshi_toupiao GROUP BY xuhao order by phones  desc
    </select>

</mapper>
